{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/heimdallr/src",
  "monorepo": true,
  "root": "apps/heimdallr",
  "compilerOptions": {
    "tsConfigPath": "tsconfig.json",
    "webpack": false
  },
  "projects": {
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.json"
      }
    },
    "health": {
      "type": "library",
      "root": "libs/health",
      "entryFile": "index",
      "sourceRoot": "libs/health/src",
      "compilerOptions": {
        "tsConfigPath": "libs/health/tsconfig.json"
      }
    },
    "heimdallr": {
      "type": "application",
      "root": "apps/heimdallr",
      "entryFile": "main",
      "sourceRoot": "apps/heimdallr/src",
      "compilerOptions": {
        "tsConfigPath": "apps/heimdallr/tsconfig.json"
      }
    },
    "midgard": {
      "type": "application",
      "root": "apps/midgard",
      "entryFile": "main",
      "sourceRoot": "apps/midgard/src",
      "compilerOptions": {
        "tsConfigPath": "apps/midgard/tsconfig.json"
      }
    },
    "mimir": {
      "type": "application",
      "root": "apps/mimir",
      "entryFile": "main",
      "sourceRoot": "apps/mimir/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mimir/tsconfig.json"
      }
    },
    "nidavellir": {
      "type": "application",
      "root": "apps/nidavellir",
      "entryFile": "main",
      "sourceRoot": "apps/nidavellir/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nidavellir/tsconfig.json"
      }
    },
    "valhalla": {
      "type": "application",
      "root": "apps/valhalla",
      "entryFile": "main",
      "sourceRoot": "apps/valhalla/src",
      "compilerOptions": {
        "tsConfigPath": "apps/valhalla/tsconfig.json"
      }
    }
  }
}
